{
  "name": "game-over",
  "displayName": "game-over Keyboard Shortcuts",
  "description": "Gamer Overhaul. Reuse muscle memory learned from games.",
  "version": "0.0.1",
  "publisher": "tonyatpeking",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "game-over.helloWorld",
        "title": "game-over.helloWorld"
      },
      {
        "command": "game-over.i",
        "title": "game-over.i"
      },
      {
        "command": "game-over.j",
        "title": "game-over.j"
      },
      {
        "command": "game-over.k",
        "title": "game-over.k"
      },
      {
        "command": "game-over.l",
        "title": "game-over.l"
      }
    ],
    "keybindings": [
      {
        "command": "game-over.i",
        "key": "i",
        "when": "game-over.moveMode"
      },
      {
        "command": "game-over.j",
        "key": "j",
        "when": "game-over.moveMode"
      },
      {
        "command": "game-over.k",
        "key": "k",
        "when": "game-over.moveMode"
      },
      {
        "command": "game-over.l",
        "key": "l",
        "when": "game-over.moveMode"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}